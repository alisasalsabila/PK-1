    "# Pengecekan Bilangan Prima"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "2cc58b27-96e4-43d9-b45d-3328e2c6c785",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Ini bilangan Prima\n"
     ]
    }
   ],
   "source": [
    "def cekPrima(n):\n",
    "    if (n <= 1):\n",
    "        return False\n",
    "    for i in range(2, n):\n",
    "        if (n % i == 0):\n",
    "            return False\n",
    "    return True\n",
    "if cekPrima(11):\n",
    "    print(\"Ini bilangan Prima\")\n",
    "else:\n",
    "    print(\"Bukan bilangan Prima\")"
   ]
